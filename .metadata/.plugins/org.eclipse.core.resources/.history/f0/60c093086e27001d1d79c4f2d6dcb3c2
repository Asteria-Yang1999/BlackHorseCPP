#include <iostream>
using namespace std;

//1.全局变量检测增强
//int a;
int a = 10;

//2.函数检测增强, 参数类型增强, 返回值检测增强, 函数调用参数检测增强
int getRectS(int w, int h)
{
	return w*h;
}

void test02()
{
	getRectS(10, 10);
}

//3.类型转换检测增强
void test03()
{
	char * p = (char*)malloc(sizeof(04)); //malloc返回值是void*
}


int main() {


	return 0;
}
